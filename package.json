{
  "name": "studykit-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass src/styles/index.scss src/index.css",
    "watch-css": "yarn build-css && node-sass src/styles/index.scss src/index.css --watch",
    "fmt": "prettier --write --jsx-bracket-same-line \"src/**/*.js\""
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bootstrap": "^4.0.0",
    "prettier": "2.6.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-markdown": "^2.5.0",
    "react-router": "3.0.5"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "react-scripts": "^1.0.7"
  }
}
